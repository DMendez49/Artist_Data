<hr />
<%= form_with(model: [@artist, @song], local: true, class: "ui form") do |f| %>
  <%= f.label :song_name %>
  <%= f.text_field :song_name, required: true, autofocus: true %>
  <br />
  <%= f.label :artist_name %>
  <%= f.text_field :artist_name, required: true %>
  <br />
  <%= f.label :hits %>
  <%= f.text_field :hits, required: true %>
  <br />
  <br />
  <%= f.submit nil, class: "ui button" %>
<% end %>
             